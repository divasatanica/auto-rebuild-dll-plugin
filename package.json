{
  "name": "@vergiss/auto-rebuild-dll-plugin",
  "version": "0.0.1",
  "description": "A Webpack plugin for rebuilding dll library",
  "main": "lib/index.js",
  "scripts": {
    "cleanup": "del-cli ./lib",
    "test": "echo \"Error: no test specified\" && exit 1",
